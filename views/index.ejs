<!DOCTYPE html>
<html>
<head>
	<title>	Movie App </title>
	<% include head %>
</head>
<body>

	<% include navbar %>

	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<h1>上映中</h1>
			</div>
			<div class="col-sm-12">
				<% parsedData.forEach((movie)=>{ %>
					<div class="col-md-3 col-sm-4 poster">
						<p><%= movie.title %></p>
						<img src="<%= imgBaseUrl + movie.poster_path %>">	
					</div>
				<% }) %>	
			</div>
		</div>
	</div>
</body>
</html>